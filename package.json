{
  "name": "@hangxingliu/ai-hub",
  "version": "0.0.1",
  "description": "Connecting multiple AI service providers with a unified entry point",
  "packageManager": "bun@1.2.19",
  "type": "module",
  "license": "LGPL-3.0-or-later",
  "module": "src/index.ts",
  "scripts": {
    "lint": "tsc",
    "gen": "bun run src/config/schema.gen.ts",
    "start": "bun run src/index.ts -c config.yaml",
    "dev": "bun --watch run src/index.ts -c config.yaml"
  },
  "dependencies": {
    "@fastify/busboy": "^3.1.1",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "http-proxy-agent": "^7.0.2",
    "https-proxy-agent": "^7.0.6",
    "socks-proxy-agent": "^8.0.5",
    "yaml": "^2.8.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "latest",
    "openai": "^5.10.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  }
}
